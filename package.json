{
  "name": "rollup-plugin-vue-demi",
  "version": "0.0.0",
  "description": "Rollup plugin for build vue sfc with vue-demi",
  "keywords": [
    "vue-demi",
    "vue"
  ],
  "license": "MIT",
  "author": "Chengjie Huang <chengjie_huang@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaygNas/rollup-plugin-vue-demi.git"
  },
  "main": "./commonjs/index.js",
  "module": "./es/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./commonjs/index.js",
      "types": "./index.d.ts"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "homepage": "TODO",
  "peerDependencies": {
    "vue-demi": "*",
    "@vue/composition-api": "^1.1.0",
    "vue": "^2.6.0 || ^3.2.0"
  },
  "dependencies": {
    "rollup-plugin-vue": "^6.0.0",
    "rollup-plugin-vue2": "^0.8.1"
  },
  "scripts": {
    "build": "rollup -c && tsc"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.19.1",
    "@types/node": "^17.0.23",
    "esbuild": "^0.14.36",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.12.0",
    "rollup": "^2.70.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^4.6.3",
    "vue-demi": "^0.12.5"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}