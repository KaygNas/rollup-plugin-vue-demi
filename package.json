{
  "name": "rollup-plugin-vue-demi",
  "private": false,
  "version": "0.0.7",
  "description": "Rollup plugin for build vue sfc with vue-demi",
  "keywords": [
    "vue-demi",
    "vue"
  ],
  "license": "MIT",
  "author": "Chengjie Huang <chengjie_huang@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaygNas/rollup-plugin-vue-demi.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/commonjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/commonjs/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./dist/*"
  },
  "sideEffects": false,
  "homepage": "https://github.com/KaygNas/rollup-plugin-vue-demi",
  "peerDependencies": {
    "@vue/composition-api": "^1.1.0",
    "vue": "^2.6.0 || ^3.2.0",
    "vue-demi": "*"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "rollup-plugin-vue": "^5.1.9",
    "vue-runtime-helpers": "^1.1.2",
    "vue-template-compiler": "^2.6.14"
  },
  "scripts": {
    "build": "rollup -c && tsc"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.19.1",
    "@types/node": "^17.0.23",
    "esbuild": "^0.14.36",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.12.0",
    "rollup": "^2.70.1",
    "rollup-plugin-esbuild": "^4.9.1",
    "typescript": "^4.6.3",
    "vue-demi": "^0.12.5"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
